    <!-- Root flavor -->
    <string name="onboarding_methods_root_root">\u25CF Doporučeno\n\u25CF Není vyžadován žádný externí počítač\n\u25CF pouze Android 11+</string>
    <string name="onboarding_methods_root_shizuku">\u25CF Doporučeno\n\u25CF Není vyžadován žádný externí počítač\n\u25CF pouze Android 11+</string>
